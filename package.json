{
  "name": "google-books-search",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/supercarboy88/Google-Books-Search"
  },
  "description": "Google Books Search",
  "main": "server.js",
  "engines": {
    "node:": "10.15.3"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "cd client && npm run build"
  },
  "author": "Nick Ying",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/supercarboy88/Google-Books-Search/issues"
  },
  "homepage": "https://github.com/supercarboy88/Google-Books-Search#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "bootstrap": "^4.4.1",
    "concurrently": "^5.0.2",
    "express": "^4.17.3",
    "if-env": "^1.0.4",
    "jquery": "^3.4.1",
    "mongoose": "^5.8.2",
    "nodemon": "^2.0.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "^3.3.0"
  }
}
